{
  "name": "enib-internships-backend",
  "version": "1.0.0",
  "description": "ENIB internships interfaces backend",
  "author": "Oscar Marie--Taillefer",
  "main": "index.js",
  "scripts": {
    "load": "node dist/fake-load.js",
    "build": "npm run build-ts && npm run lint && npm run init",
    "serve": "node dist/server.js",
    "watch-node": "nodemon dist/server.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "test": "jest --coverage --verbose --runInBand --detectOpenHandles",
    "build-ts": "tsc --build ./tsconfig.build.json",
    "watch-ts": "tsc --build ./tsconfig.build.json -w",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "init": "node ./devops/scripts/init.js",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect dist/server.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\""
  },
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "chalk": "^2.4.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.1.0",
    "errorhandler": "^1.5.0",
    "express": "^4.16.4",
    "express-flash": "0.0.2",
    "express-session": "^1.17.0",
    "express-validator": "^6.2.0",
    "http-status-codes": "^1.3.2",
    "lodash": "^4.17.15",
    "lusca": "^1.6.1",
    "method-override": "^3.0.0",
    "moment": "^2.24.0",
    "mysql2": "^1.7.0",
    "nodemailer": "^6.3.1",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.7",
    "sequelize": "^5.19.6",
    "shelljs": "^0.8.3",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/bcrypt-nodejs": "0.0.30",
    "@types/commander": "^2.12.2",
    "@types/compression": "1.0.1",
    "@types/cors": "^2.8.6",
    "@types/dotenv": "^6.1.1",
    "@types/errorhandler": "0.0.32",
    "@types/express": "^4.16.1",
    "@types/express-flash": "0.0.1",
    "@types/express-session": "^1.15.15",
    "@types/express-validator": "^3.0.0",
    "@types/faker": "^4.1.6",
    "@types/jest": "^24.0.19",
    "@types/lodash": "^4.14.144",
    "@types/lusca": "^1.5.0",
    "@types/method-override": "0.0.31",
    "@types/moment": "^2.13.0",
    "@types/nodemailer": "^6.2.1",
    "@types/request-promise-native": "^1.0.17",
    "@types/sequelize": "^4.28.6",
    "@types/supertest": "^2.0.7",
    "chai": "^4.2.0",
    "commander": "^3.0.2",
    "concurrently": "^5.0.0",
    "faker": "^4.1.0",
    "jest": "^24.7.1",
    "node-sass": "^4.11.0",
    "nodemon": "^1.19.3",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.2",
    "tslint": "^5.15.0",
    "typescript": "3.6.4"
  }
}
