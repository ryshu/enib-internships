{
  "name": "enib-internships-backend",
  "version": "1.0.0",
  "description": "ENIB internships interfaces backend",
  "author": "Oscar Marie--Taillefer",
  "main": "index.js",
  "scripts": {
    "load": "node dist/fake-load.js",
    "build": "npm run build-ts && npm run lint && npm run init",
    "serve": "node dist/server.js",
    "watch-node": "nodemon dist/server.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "test": "node devops/scripts/test.init.js && jest --coverage --verbose --runInBand --detectOpenHandles",
    "build-ts": "tsc --build ./tsconfig.build.json",
    "watch-ts": "tsc --build ./tsconfig.build.json -w",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "init": "node ./devops/scripts/init.js",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect dist/server.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\"",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "cas-authentication": "0.0.8",
    "chalk": "^3.0.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "email-templates": "^6.0.3",
    "errorhandler": "^1.5.0",
    "express": "^4.16.4",
    "express-flash": "0.0.2",
    "express-session": "^1.17.0",
    "express-socket.io-session": "^1.3.5",
    "express-validator": "^6.3.0",
    "http-status-codes": "^1.4.0",
    "lodash": "^4.17.15",
    "lusca": "^1.6.1",
    "method-override": "^3.0.0",
    "moment": "^2.24.0",
    "multer": "^1.4.2",
    "mysql2": "^2.1.0",
    "nodemailer": "^6.4.2",
    "pug": "^2.0.4",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.8",
    "sequelize": "^5.21.3",
    "shelljs": "^0.8.3",
    "socket.io": "^2.3.0",
    "winston": "^3.2.1",
    "snyk": "^1.265.0"
  },
  "devDependencies": {
    "@types/bcrypt-nodejs": "0.0.30",
    "@types/commander": "^2.12.2",
    "@types/compression": "1.0.1",
    "@types/cors": "^2.8.6",
    "@types/dotenv": "^8.2.0",
    "@types/email-templates": "^6.0.2",
    "@types/errorhandler": "0.0.32",
    "@types/express": "^4.17.2",
    "@types/express-flash": "0.0.1",
    "@types/express-session": "^1.15.16",
    "@types/express-socket.io-session": "^1.3.2",
    "@types/express-validator": "^3.0.0",
    "@types/faker": "^4.1.8",
    "@types/jest": "^24.0.24",
    "@types/lodash": "^4.14.149",
    "@types/lusca": "^1.5.0",
    "@types/method-override": "0.0.31",
    "@types/moment": "^2.13.0",
    "@types/multer": "^1.3.10",
    "@types/nodemailer": "^6.4.0",
    "@types/pug": "^2.0.4",
    "@types/request-promise-native": "^1.0.17",
    "@types/sequelize": "^4.28.8",
    "@types/socket.io": "^2.1.4",
    "@types/supertest": "^2.0.7",
    "chai": "^4.2.0",
    "commander": "^4.0.1",
    "concurrently": "^5.0.2",
    "faker": "^4.1.0",
    "jest": "^24.7.1",
    "node-sass": "^4.13.0",
    "nodemon": "^1.19.4",
    "supertest": "^4.0.2",
    "ts-jest": "^24.2.0",
    "tslint": "^5.20.1",
    "typescript": "3.7.4"
  },
  "snyk": true
}
